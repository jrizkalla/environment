{
	"name": "theme-jonokai-vscode",
	"displayName": "Jonokai",
	"description": "Professional theme and matching icons, from the author of the original Monokai color scheme.",
	"publisher": "John Rizkalla",
	"version": "1.2.0",
	"engines": {
		"vscode": "^1.14.0"
	},
	"categories": [
		"Themes"
	],
	"capabilities": {
		"untrustedWorkspaces": {
			"supported": true
		}
	},
	"contributes": {
		"commands": [
			{
				"command": "monokai_pro.select_theme",
				"title": "Monokai Pro: select theme"
			},
			{
				"command": "monokai_pro.activate_icons",
				"title": "Monokai Pro: activate icons"
			}
		],
		"themes": [
			{
				"label": "Jonokai",
				"uiTheme": "vs-dark",
				"path": "./themes/Monokai Pro.json"
			},
			{
				"label": "Jonokai (Filter Octagon)",
				"uiTheme": "vs-dark",
				"path": "./themes/Monokai Pro (Filter Octagon).json"
			},
			{
				"label": "Jonokai (Filter Ristretto)",
				"uiTheme": "vs-dark",
				"path": "./themes/Monokai Pro (Filter Ristretto).json"
			},
			{
				"label": "Jonokai (Filter Spectrum)",
				"uiTheme": "vs-dark",
				"path": "./themes/Monokai Pro (Filter Spectrum).json"
			},
			{
				"label": "Jonokai (Filter Machine)",
				"uiTheme": "vs-dark",
				"path": "./themes/Monokai Pro (Filter Machine).json"
			},
			{
				"label": "Jonokai Classic",
				"uiTheme": "vs-dark",
				"path": "./themes/Monokai Classic.json"
			}
		],
		"iconThemes": [
			{
				"label": "Jonokai Icons",
				"id": "Monokai Pro Icons",
				"path": "./icon-themes/Monokai Pro icon-theme.json"
			},
			{
				"label": "Jonokai Monochrome Icons",
				"id": "Monokai Pro Monochrome Icons",
				"path": "./icon-themes/Monokai Pro monochrome icon-theme.json"
			},
			{
				"label": "Jonokai (Filter Octagon) Icons",
				"id": "Monokai Pro (Filter Octagon) Icons",
				"path": "./icon-themes/Monokai Pro (Filter Octagon) icon-theme.json"
			},
			{
				"label": "Jonokai (Filter Octagon) Monochrome Icons",
				"id": "Monokai Pro (Filter Octagon) Monochrome Icons",
				"path": "./icon-themes/Monokai Pro (Filter Octagon) monochrome icon-theme.json"
			},
			{
				"label": "Jonokai (Filter Ristretto) Icons",
				"id": "Monokai Pro (Filter Ristretto) Icons",
				"path": "./icon-themes/Monokai Pro (Filter Ristretto) icon-theme.json"
			},
			{
				"label": "Jonokai (Filter Ristretto) Monochrome Icons",
				"id": "Monokai Pro (Filter Ristretto) Monochrome Icons",
				"path": "./icon-themes/Monokai Pro (Filter Ristretto) monochrome icon-theme.json"
			},
			{
				"label": "Jonokai (Filter Spectrum) Icons",
				"id": "Monokai Pro (Filter Spectrum) Icons",
				"path": "./icon-themes/Monokai Pro (Filter Spectrum) icon-theme.json"
			},
			{
				"label": "Jonokai (Filter Spectrum) Monochrome Icons",
				"id": "Monokai Pro (Filter Spectrum) Monochrome Icons",
				"path": "./icon-themes/Monokai Pro (Filter Spectrum) monochrome icon-theme.json"
			},
			{
				"label": "Jonokai (Filter Machine) Icons",
				"id": "Monokai Pro (Filter Machine) Icons",
				"path": "./icon-themes/Monokai Pro (Filter Machine) icon-theme.json"
			},
			{
				"label": "Jonokai (Filter Machine) Monochrome Icons",
				"id": "Monokai Pro (Filter Machine) Monochrome Icons",
				"path": "./icon-themes/Monokai Pro (Filter Machine) monochrome icon-theme.json"
			},
			{
				"label": "Jonokai Classic Icons",
				"id": "Monokai Classic Icons",
				"path": "./icon-themes/Monokai Classic icon-theme.json"
			},
			{
				"label": "Jonokai Classic Monochrome Icons",
				"id": "Monokai Classic Monochrome Icons",
				"path": "./icon-themes/Monokai Classic monochrome icon-theme.json"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Monokai Pro Settings",
			"properties": {
				"monokaiPro.fileIconsMonochrome": {
					"type": "boolean",
					"default": false,
					"description": "makes the file icons monochrome"
				}
			}
		}
	},
	"extensionKind": [
		"ui",
		"workspace"
	],
	"galleryBanner": {
		"color": "#fdf9f3",
		"theme": "light"
	},
	"icon": "icon.png",
	"keywords": [
		"beautiful functionality",
		"focus",
		"minimal",
		"icons",
		"syntax highlighting"
	],
	"main": "./js/main",
	"browser": "./js/browser",
	"license": "SEE LICENSE IN LICENSE",
	"activationEvents": [
		"onStartupFinished"
	],
	"qna": false
}